    <xs:complexType name="AssertionTypeRCV">
        <xs:annotation>
            <xs:documentation>Assertion is used to represent the type of relationship between the
                trait set and the measure set. This is stored in
                GTR.clinvar.measure_trait.relat_type.    </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Type" type="AssertionTypeAttr" use="required"/>
    </xs:complexType>
    <xs:complexType name="AssertionTypeSCV">
        <xs:annotation>
            <xs:documentation>Assertion is used to represent the type of relationship between the
                trait set and the measure set. This is stored in
                GTR.clinvar.measure_trait.relat_type.    </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Type" type="AssertionTypeAttrSCV" use="required"/>
    </xs:complexType>
    <xs:simpleType name="ZygosityType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Homozygote"/>
            <xs:enumeration value="SingleHeterozygote"/>
            <xs:enumeration value="CompoundHeterozygote"/>
            <xs:enumeration value="Hemizygote"/>
