    <xs:complexType name="typeRecordHistory">
        <xs:annotation>
            <xs:documentation>     A structure to support reporting of an accession, its version,
                the date its status changed, and text describing that change.    </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Comment" type="CommentType"/>
